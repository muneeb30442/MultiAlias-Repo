    "# model-dev/model_training.ipynb\n",
    "from sklearn.linear_model import LogisticRegression\n",
    "import pickle\n",
    "\n",
    "X = [[0, 0], [1, 1]]\n",
    "y = [0, 1]\n",
    "\n",
    "model = LogisticRegression()\n",
    "model.fit(X, y)\n",
    "\n",
    "with open('../backend/model/model.pkl', 'wb') as f:\n",
    "    pickle.dump(model, f)"
   ]
  }
